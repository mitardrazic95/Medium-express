{
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.js", "use": "@vercel/node" }
  ],  
  
  "projectId":"prj_q8e2cepmbGPnrZynboI8XKUBu3F0",
    
    "orgId":"oPJwCj4RSzjz4zfw4lTXHnEd",

    "headers": [
      {
        "source": "/service-worker.js",
        "headers" : [
          {
            "key" : "Cache-Control",
            "value" : "public, max-age=0, must-revalidate"
          }
        ]
      },
      {
        "source": "/(.*)",
        "headers" : [
          {
            "key" : "X-Content-Type-Options",
            "value" : "nosniff"
          },
          {
            "key" : "X-Frame-Options",
            "value" : "DENY"
          },
          {
            "key" : "X-XSS-Protection",
            "value" : "1; mode=block"
          }
        ]
      },
      {
        "source": "/:path*",
        "has": [
          {
            "type": "query",
            "key": "authorized"
          }
        ],
        "headers": [
          {
            "key": "x-authorized",
            "value": "true"
          }
        ]
      }
    ]
  }